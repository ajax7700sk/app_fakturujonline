<section id="tax-document-item-template" style="display: none;">
    <table>
        <tbody>
            <tr>
                <td class="td-name">
                    <div>
                        <input
                                name="lineItems[_index_][name]"
                                type="text"
                                class="form-control js-name">
                    </div>
                </td>
                <td class="td-quantity">
                    <div>
                        <input
                                name="lineItems[_index_][quantity]"
                                type="number"
                                class="form-control js-quantity"
                                value="1">
                    </div>
                </td>
                <td class="td-unit">
                    <div class="form-floating">
                        <select name="lineItems[_index_][unit]"
                                class="form-select text-muted"
                                aria-label="M.J">
                            <option value="" aria-labelledby="M.J" hidden="">M.J</option>
                            <option value="ks" aria-labelledby="ks" label="ks">ks</option>
                            <option value="hod." aria-labelledby="hod" label="hod">hod</option>
                            <option value="deň" aria-labelledby="deň" label="deň">deň</option>
                            <option value="liter" aria-labelledby="liter" label="liter">liter</option>
                            <option value="Kg" aria-labelledby="Kg" label="Kg">Kg</option>
                            <option value="g" aria-labelledby="g" label="g">g</option>
                            <option value="m" aria-labelledby="m" label="m">m</option>
                            <option value="km" aria-labelledby="km" label="km">km</option>
                            <option value="m²" aria-labelledby="m²" label="m²">m²</option>
                            <option value="m³" aria-labelledby="m³" label="m³">m³</option>
                        </select>
                </td>
                <td class="td-unitPriceTaxExcl">
                    <div class="form-floating">
                        <input
                                name="lineItems[_index_][unitPriceTaxExcl]"
                                type="number"
                                class="form-control js-unit-price-tax-excl"
                                placeholder=""
                                value="0">
                    </div>
                </td>
                <td class="js-taxable td-taxRate">
                    <div class="form-floating">
                        <input name="lineItems[_index_][taxRate]"
                               type="number"
                               class="form-control js-tax-rate"
                               placeholder=""
                               min="0"
                               value="0">
                    </div>
                </td>
                <td class="js-taxable td-priceTaxExcl">
                    <div class="form-floating">
                        <input
                                type="number"
                                class="form-control js-item-excl-tax"
                                placeholder=""
                                disabled
                                value="0"
                        />
                        <div class="error-msg"></div>
                    </div>
                </td>
                <td class="td-priceTaxIncl">
                    <div class="form-floating">
                        <input
                               type="number"
                               class="form-control js-item-total"
                               placeholder=""
                               disabled
                               value="0">
                    </div>
                </td>
                <td>
                    <button
                            type="button"
                            class="btn btn-light-danger me-3 js-remove-item">-</button>
                </td>
            </tr>
        </tbody>
    </table>
</section>